{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": false,
        "IncludeScopes": false,
        "UseUtcTimestamp": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff "
      }
    }
  },
  "Service": {
    "AssemblyPath": ""
  },
  "Worker": {
    "Concurrency": 5
  },
  "Requests": {
    "QueueType": "",
    "QueueName": "requests",
    "ConnectionString": "",
    "MessageLease": 60
  },
  "Responses": {
    "QueueType": "",
    "QueueName": "responses",
    "ConnectionString": "",
    "MessageLease": 60
  }
}
